{
    "id": "/confluent/schema-registry",
    "instances": 1,
    "cpus": 0.5,
    "mem": 128,
    "disk": 100,
    "constraints": [["hostname", "CLUSTER", "172.31.2.12"]],
    "container": {
	"type": "DOCKER",
	"docker": {
	    "image": "172.31.2.11:31000/confluent/schema-registry:1.0.1",
	    "parameters": [],
	    "network": "BRIDGE",
	    "portMappings": [
		{ "containerPort": 8081, "hostPort": 0 }
	    ],
	    "forcePullImage": true
	},
	"volumes": []
    },
    "healthChecks": [{
	"protocol": "HTTP",
	"path": "/",
	"portIndex": 0,
	"gracePeriodSeconds": 5,
	"intervalSeconds": 20,
	"maxConsecutiveFailures": 3,
	"backoffSeconds":1,
	"backoffFactor": 1.15,
	"timeoutSeconds": 10
    }],
    "ports": [ 0 ]
}
